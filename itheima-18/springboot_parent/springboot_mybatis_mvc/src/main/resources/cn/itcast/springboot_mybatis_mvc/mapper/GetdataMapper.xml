<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.springboot_mybatis_mvc.mapper.GetdataMapper">

    <!--根据id 查询用户信息-->
    <select id="findByPlayer_id" resultType="cn.itcast.springboot_mybatis_mvc.entity.ActivityEntity">
        select * from activity where player_id = #{player_id}
    </select>
</mapper>
